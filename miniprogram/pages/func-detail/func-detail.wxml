<scroll-view class="scrollarea" scroll-y type="list">
  <view class="container">
    <view class="title">{{name}}函数</view>
    <view class="wrapper">
      <entry-cell title="{{name}}" description="{{description}}"></entry-cell>
      <view class="spinner-container">
        <spinner wx:if="{{requesting}}"/>
      </view>
    </view>
  </view>
</scroll-view>